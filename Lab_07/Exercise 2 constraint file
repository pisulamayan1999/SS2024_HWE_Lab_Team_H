## Clock signal
set_property PACKAGE_PIN W5 [get_ports {CLK}]
set_property IOSTANDARD LVCMOS33 [get_ports {CLK}]

## Reset signal
set_property PACKAGE_PIN U18 [get_ports {RST}]
set_property IOSTANDARD LVCMOS33 [get_ports {RST}]

## Enable signal
set_property PACKAGE_PIN U19 [get_ports {EN}]
set_property IOSTANDARD LVCMOS33 [get_ports {EN}]

## LED outputs
set_property PACKAGE_PIN G6 [get_ports {LED_Out[0]}]
set_property PACKAGE_PIN F6 [get_ports {LED_Out[1]}]
set_property PACKAGE_PIN E6 [get_ports {LED_Out[2]}]
set_property PACKAGE_PIN D6 [get_ports {LED_Out[3]}]
set_property PACKAGE_PIN C6 [get_ports {LED_Out[4]}]
set_property PACKAGE_PIN B6 [get_ports {LED_Out[5]}]
set_property PACKAGE_PIN A6 [get_ports {LED_Out[6]}]
set_property PACKAGE_PIN B5 [get_ports {LED_Out[7]}]
set_property PACKAGE_PIN C5 [get_ports {LED_Out[8]}]
set_property PACKAGE_PIN D5 [get_ports {LED_Out[9]}]
set_property PACKAGE_PIN E5 [get_ports {LED_Out[10]}]
set_property PACKAGE_PIN F5 [get_ports {LED_Out[11]}]
set_property IOSTANDARD LVCMOS33 [get_ports {LED_Out[*]}]
